{
  "name": "immersa-3d-scene-viewer",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered 2D to 3D scene reconstruction using React Three Fiber and TensorFlow.js",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all",
    "ios >= 15",
    "safari >= 15",
    "chrome >= 100",
    "firefox >= 100",
    "edge >= 100"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "lint:strict": "eslint src --max-warnings 0 && tsc --noEmit",
    "verify": "npm run lint:strict && vite build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "biome:check": "biome check src",
    "biome:fix": "biome check --write src",
    "biome:organize": "biome organize src",
    "biome:lint": "biome lint src",
    "biome:format": "biome format src",
    "clean": "rimraf dist node_modules/.vite",
    "deps:update": "npm update"
  },
  "keywords": [
    "react",
    "three.js",
    "3d",
    "depth-estimation",
    "ai",
    "gemini",
    "tensorflow"
  ],
  "dependencies": {
    "@google/genai": "^1.35.0",
    "@mediapipe/face_detection": "^0.4.1646425229",
    "@react-three/drei": "^10.0.0",
    "@react-three/fiber": "^9.0.0",
    "@sentry/react": "^10.36.0",
    "@tensorflow-models/depth-estimation": "^0.0.4",
    "@tensorflow-models/face-detection": "^1.0.3",
    "@tensorflow/tfjs": "^4.22.0",
    "hls.js": "^1.6.15",
    "lucide-react": "^0.562.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "three": "^0.182.0",
    "three-stdlib": "^2.36.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.0.0",
    "vite-plugin-glsl": "^1.5.5",
    "wait-on": "^8.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "three": "^0.182.0",
    "tar": "^7.5.4"
  }
}
